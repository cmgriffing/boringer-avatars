{
  "name": "@boringer-avatars/core",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "mitosis": "mitosis",
    "build:mitosis:plugins": "tsc --project tsconfig.plugins.json",
    "build:mitosis:main": "cp ./scripts/avatar.main.mustache ./src/avatar.lite.tsx && pnpm mitosis build",
    "build:mitosis:vue2": "cp ./scripts/avatar.vue2.mustache ./src/avatar.lite.tsx && pnpm mitosis build --config=mitosis.vue2.config.js",
    "build:mitosis": "npm-run-all -s build:mitosis:main build:mitosis:vue2",
    "build:compile:main": "ts-node --project=tsconfig.json ./scripts/new-compile.ts",
    "build:compile:vue2": "ts-node --project=tsconfig.json ./scripts/compile-vue2.ts",
    "build:compile": "npm-run-all -s build:compile:main build:compile:vue2",
    "build:new": "npm-run-all -s build:mitosis:plugins build:mitosis build:compile"
  },
  "devDependencies": {
    "@builder.io/mitosis": "^0.0.82",
    "@builder.io/mitosis-cli": "^0.0.40",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/mustache": "^4.2.1",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "packageManager": "pnpm@7.2.1"
}
